    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.13.2",
    "webpack-split-by-path": "0.0.10",
    "webpack-uglify-js-plugin": "^1.1.8",
    "whatwg-fetch": "^1.0.0"
  }
}
