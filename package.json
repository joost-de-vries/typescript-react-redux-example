{
  "name": "typescript-react-redux-starter",
  "version": "0.1.0",
  "description": "A boiler-plate setup for TypeScript/React/Redux applications",
  "license": "MIT",
  "engines": {
    "node": "6"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack",
    "clean": "rimraf dist/",
    "dev:server": "cross-env NODE_ENV=development PORT=3000 nodemon server/node-server.js",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server -d --inline --hot --progress --no-info",
    "dev": "concurrently -r \"npm run dev:client\" \"npm run dev:server\"",
    "lint": "npm run lint-ts && npm run lint-css",
    "lint-css": "stylelint \"./src/**/*.css\"",
    "lint-ts": "tslint \"src/**/*.tsx\" \"src/**/*.ts\" \"test/**/*.tsx\" \"test/**/*.ts\"",
    "postinstall": "npm run build",
    "start": "cross-env NODE_ENV=production node server/node-server.js",
    "pretest": "npm run lint",
    "test": "karma start --singleRun true",
    "test:watch": "karma start --singleRun false"
  },
  "keywords": [
    "react",
    "redux",
    "typescript",
    "starter",
    "seed"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rangle/typescript-react-redux-starter.git"
  },
  "bugs": {
    "url": "https://github.com/rangle/rangle-starter/issues"
  },
  "dependencies": {
    "express": "^4.13.4",
    "helmet": "^2.0.0",
    "http-proxy": "^1.13.2",
    "winston": "^2.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel-polyfill": "^6.9.1",
    "basscss": "^8.0.2",
    "basscss-addons": "^1.0.0",
    "basscss-basic": "^1.0.0",
    "body-parser": "^1.15.1",
    "classnames": "^2.2.3",
    "concurrently": "^2.0.0",
    "copy-webpack-plugin": "4.0.0",
    "cross-env": "3.1.3",
    "css-loader": "^0.23.1",
    "cssnano": "^3.5.2",
    "denodeify": "^1.2.1",
    "enzyme": "^2.4.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "history": "^4.2.0",
    "html-webpack-plugin": "^2.9.0",
    "immutable": "^3.8.1",
    "imports-loader": "^0.6.5",
    "jasmine-core": "^2.4.1",
    "json-loader": "^0.5.4",
    "karma": "1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-typescript": "2.1.2",
    "nodemon": "^1.9.2",
    "object-assign": "^4.0.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "postcss-cssnext": "^2.4.0",
    "postcss-import": "^8.0.2",
    "postcss-loader": "^0.13.0",
    "postcss-modules-local-by-default": "^1.0.2",
    "raw-loader": "^0.5.1",
    "react": "^15.3.0",
    "react-addons-test-utils": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.4",
    "react-router": "^2.5.2",
    "react-router-redux": "^4.0.5",
    "redux": "^3.6.0",
    "redux-form": "^5.3.2",
    "redux-localstorage": "^0.4.0",
    "redux-logger": "^2.6.0",
    "redux-thunk": "^2.1.0",
    "require-hacker": "^2.1.3",
    "rimraf": "^2.4.3",
    "source-map-support": "^0.4.0",
    "style-loader": "^0.13.0",
    "stylelint": "^7.5.0",
    "stylelint-webpack-plugin": "^0.4.0",
    "ts-helpers": "^1.1.1",
    "tslint": "^3.15.1",
    "tslint-eslint-rules": "2.1.0",
    "tslint-loader": "^2.1.5",
    "ts-loader": "1.2.0",
    "typescript": "2.0.9",
    "@types/classnames": "0.0.31",
    "@types/enzyme": "2.5.37",
    "@types/jasmine": "2.5.37",
    "@types/node": "6.0.46",
    "@types/object-assign": "4.0.30",
    "@types/react": "0.14.47",
    "@types/react-addons-test-utils": "0.14.15",
    "@types/react-dom": "0.14.18",
